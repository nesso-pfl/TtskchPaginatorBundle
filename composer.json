{
    "name": "ttskch/paginator-bundle",
    "description": "The most thin and simple paginator bundle for Symfony",
    "keywords": ["symfony", "pager", "pagination", "paginator", "sortable", "customizable", "searchable"],
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ttskch\\PaginatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Ttskch\\PaginatorBundle\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.3|>=7.2.5",
        "symfony/framework-bundle": "^4.0|^5.0|^6.0",
        "symfony/twig-bundle": "^4.0|^5.0|^6.0",
        "symfony/form": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "php": ">=7.2.5",
        "phpunit/phpunit": "^8.5",
        "symfony/framework-bundle": "^5.1|^6.0",
        "symfony/phpunit-bridge": "^4.0|^5.0|^6.0",
        "symfony/yaml": "^4.0|^5.0|^6.0",
        "symfony/browser-kit": "^4.0|^5.0|^6.0",
        "doctrine/orm": "^2.8"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
